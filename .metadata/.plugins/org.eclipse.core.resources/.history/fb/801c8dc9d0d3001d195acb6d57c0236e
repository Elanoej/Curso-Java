package application;

import java.util.Iterator;

public class Program {

	public static void main(String[] args) {
		long inicio = System.currentTimeMillis();
		concatString();

	}
	
	private static void concatString(int tamanho) {
		String texto = "";
		for (int i = 0; i < tamanho; i++) {
			texto += i;
		}
	}
}
